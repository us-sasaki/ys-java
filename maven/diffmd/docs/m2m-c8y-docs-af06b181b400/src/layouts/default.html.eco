</head>
<body>
  <%- @partial("header") %>
<div class="container">
  <div class="row row-offcanvas row-offcanvas-left">

    <div class="col-xs-6 col-sm-3 sidebar-offcanvas hidden-print" id="sidebar" role="navigation">

      <div class="left-nav">

        <div id="docs-nav">
          <% for section in @sections:%>
              <% $first = 'collapsed' %>
              <% for page in @getCollection("html").findAllLive({relativeOutDirPath: section.folder }, [ order: 1, title: 1 ] ).toJSON():%>
                <% if page.id is @document.id then $in = 'in' %>
                <% if page.id is @document.id then $first = '' %>
              <% end %>

            <div class="panel">
              <a data-toggle="collapse" data-parent="#docs-nav" class="top_docs <%= $first %>" href="#<%= section.slug %>">
                <h4><span class="glyphicon <%= section.icon %>"></span> <%= section.title %><span class="caret"></span></h4>
              </a>

              <div id="<%= section.slug %>" class="collapse <%= $in %>">
                <% $in = '' %>
                <ul class="list-group">
                  <% for page in @getCollection("html").findAllLive({relativeOutDirPath: section.folder }, [ order: 1, title: 1 ] ).toJSON():%>
                    <li class="<%= if page.id is @document.id then 'active' %> list-group-item">
                      <a href="<%= page.url%>"> <%= page.title || 'no title' %></a>
                    </li>
                  <% end %>
                </ul>
              </div>
            </div>
          <% end %>
        </div>

      </div>
    </div>

    <div class="col-xs-12 col-sm-9">
          <button type="button" class="navbar-toggle docs-nav-toggle" data-toggle="offcanvas">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      <h1 class="section-title"><%= @document.title %></h1>
      <div class="content">
         <%- @partial('toc.html.eco', {tocItems: @document.tableOfContents}) %>
         <%- @content %>
      </div>
    </div>

  </div>

</div>
<footer>
  <div class="container">
    <hr>
