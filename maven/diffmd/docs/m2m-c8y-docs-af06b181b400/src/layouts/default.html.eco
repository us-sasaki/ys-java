   <link rel="shortcut icon" href="/guides/favicon.ico"/>

  <script>
        var isInIFrame = window.location !== window.parent.location;
        if(isInIFrame){

          document.write('<link href="/guides/css/docs-iframe-styles.css" rel="stylesheet" id="cumulocity-style-css" type="text/css" media="all" />');
        }else{
          document.write('<link href="/guides/less/docs-styles.css" rel="stylesheet" id="cumulocity-style-css" type="text/css" media="all" />');
        }
  </script>


    <link rel="stylesheet" href="//yandex.st/highlightjs/8.0/styles/solarized_dark.min.css">
    <%- @getBlock("styles").toHTML() %>
