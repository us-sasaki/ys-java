{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "parser": "babel-eslint",
    "parserOptions": {
    	"ecmaVersion": 2019
    },
    "env": {
    	"browser": true,
    	"jasmine": true
    },
    "globals": {
    	"Card": true,
    	"Bid": true
    },
    "rules": {
    	"semi": "error",
    	"no-undef": "off",
    	"no-constant-condition": ["error", {"checkLoops": false}]
    }
}
